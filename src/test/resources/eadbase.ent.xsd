<!-- ************************************************************ -->
<!-- ************************************************************ -->
<!-- CONTENT MODEL COMPONENTS -->
<!-- ************************************************************ -->
<!-- ************************************************************ -->
<!-- BASIC COMPONENTS -->
<!-- RENDER ELEMENTS -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="m.render" abstract="true" substitutionGroup="m.phrase.bare"/>
  <xs:element name="m.ptrs" abstract="true" substitutionGroup="m.phrase.bare"/>
  <xs:element name="m.refs" abstract="true"/>
  <xs:element name="m.access" abstract="true" substitutionGroup="m.data"/>
  <xs:element name="m.data" abstract="true"/>
  <!-- ************************************************************ -->
  <!-- PHRASE COMPONENT -->
  <!-- ************************************************************ -->
  <!--
    Phrase component models are widely used groups of elements.
    The models are defined progessively, the models progress from
    the least complex to the most complex.
  -->
  <!-- BARE PHRASE -->
  <xs:element name="m.phrase.bare" abstract="true" substitutionGroup="m.phrase.basic.norefs"/>
  <!-- BASIC PHRASE -->
  <!--
    m.phrase.basic.norefs is used in content model of bibref
    and archref, and in the model used by ref and archref,
    where inclusion of ref elements is selective
  -->
  <xs:element name="m.phrase.basic.norefs" abstract="true"/>
  <xs:group name="m.phrase.basic">
    <xs:choice>
      <xs:element ref="m.phrase.basic.norefs"/>
      <xs:element ref="m.refs"/>
    </xs:choice>
  </xs:group>
  <!-- PHRASE PLUS ELEMENTS -->
  <!--
    m.phrase.plus.norefs used in content model of
    para.content.norefs which is used in content model of ref
    and archref, where inclusion of ref elements is selective.
  -->
  <xs:group name="m.phrase.plus.norefs">
    <xs:choice>
      <xs:element ref="m.phrase.basic.norefs"/>
      <xs:element ref="m.data"/>
    </xs:choice>
  </xs:group>
  <xs:group name="m.phrase.plus">
    <xs:choice>
      <xs:element ref="m.phrase.basic.norefs"/>
      <xs:element ref="m.data"/>
      <xs:element ref="m.refs"/>
    </xs:choice>
  </xs:group>
  <!-- ************************************************************ -->
  <!-- INTERMEDIATE-LEVEL COMPONENTS -->
  <!-- ************************************************************ -->
  <!-- Elements that can occur within or among paragraphs. -->
  <xs:group name="m.inter.noquote">
    <xs:choice>
      <xs:element ref="address"/>
      <xs:element ref="chronlist"/>
      <xs:element ref="list"/>
      <xs:element ref="note"/>
      <xs:element ref="table"/>
    </xs:choice>
  </xs:group>
  <xs:group name="m.inter">
    <xs:choice>
      <xs:group ref="m.inter.noquote"/>
      <xs:element ref="blockquote"/>
    </xs:choice>
  </xs:group>
  <!-- ************************************************************ -->
  <!-- PARAGRAPH-LEVEL COMPONENTS -->
  <!-- ************************************************************ -->
  <!-- Elements that can occur at the same level as a paragraph. -->
  <xs:group name="m.blocks">
    <xs:choice>
      <xs:group ref="m.inter"/>
      <xs:element ref="p"/>
    </xs:choice>
  </xs:group>
  <!-- m.block.noquote used in content model of blockquote -->
  <xs:group name="m.blocks.noquote">
    <xs:choice>
      <xs:group ref="m.inter.noquote"/>
      <xs:element ref="p"/>
    </xs:choice>
  </xs:group>
  <!-- ************************************************************ -->
  <!-- COMPONENTS FOR DESCRIPTIVE ELEMENTS -->
  <!-- ************************************************************ -->
  <!-- DESCRIPTIVE IDENTIFICATION ELEMENTS -->
  <xs:group name="m.did">
    <xs:choice>
      <xs:element ref="abstract"/>
      <xs:element ref="physdesc"/>
      <xs:element ref="note"/>
      <xs:element ref="repository"/>
      <xs:element ref="origination"/>
      <xs:element ref="unitdate"/>
      <xs:element ref="unitid"/>
      <xs:element ref="unittitle"/>
      <xs:element ref="container"/>
      <xs:element ref="physloc"/>
      <xs:element ref="dao"/>
      <xs:element ref="daogrp"/>
    </xs:choice>
  </xs:group>
  <!-- DESCRIPTIVE ELEMENTS -->
  <!-- m.desc.base used in dentry -->
  <xs:element name="m.desc.base" abstract="true"/>
  <xs:group name="m.desc.full">
    <xs:choice>
      <xs:element ref="m.desc.base"/>
      <xs:element ref="dsc"/>
      <xs:element ref="dao"/>
      <xs:element ref="daogrp"/>
      <xs:element ref="note"/>
    </xs:choice>
  </xs:group>
  <!-- ADMINISTRATIVE INFORMATION -->
  <xs:element name="m.admininfo" abstract="true"/>
  <!-- ************************************************************ -->
  <!-- INSIDE PARAGRAPHS AND NOTES -->
  <!-- ************************************************************ -->
  <!--
    PARAGRAPH CONTENT
    Elements that can occur inside a
    paragraph
  -->
  <xs:group name="m.para.content">
    <xs:choice>
      <xs:group ref="m.phrase.plus"/>
      <xs:group ref="m.inter"/>
    </xs:choice>
  </xs:group>
  <!--
    para.content.norefs used in content model of ref elements,
    where inclusion of ref elements is selective
  -->
  <xs:group name="m.para.content.norefs">
    <xs:choice>
      <xs:group ref="m.phrase.plus.norefs"/>
      <xs:group ref="m.inter"/>
    </xs:choice>
  </xs:group>
  <!-- ************************************************************ -->
  <!-- INSIDE TABLE ENTRY -->
  <!-- ************************************************************ -->
  <xs:group name="m.table.entry">
    <xs:choice>
      <xs:group ref="m.phrase.plus"/>
      <xs:element ref="address"/>
      <xs:element ref="list"/>
      <xs:element ref="note"/>
    </xs:choice>
  </xs:group>
  <!-- ************************************************************ -->
  <!-- COMPLETE CONTENT MODELS -->
  <!-- ************************************************************ -->
  <!-- HEADS AND PARAGRAPHS -->
  <xs:complexType name="cm.head.n.para">
    <xs:sequence>
      <xs:element minOccurs="0" ref="head"/>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="m.blocks"/>
    </xs:sequence>
  </xs:complexType>
  <!-- ************************************************************ -->
  <!-- PARAMETER ENTITIES FOR ATTRIBUTE LISTS -->
  <!-- ************************************************************ -->
  <!-- ************************************************************ -->
  <!-- PARAMETER ENTITIES FOR CLUSTERS OF VALUES -->
  <!-- ************************************************************ -->
  <xs:simpleType name="av.level">
    <xs:restriction base="xs:token">
      <xs:enumeration value="series"/>
      <xs:enumeration value="collection"/>
      <xs:enumeration value="file"/>
      <xs:enumeration value="fonds"/>
      <xs:enumeration value="item"/>
      <xs:enumeration value="otherlevel"/>
      <xs:enumeration value="recordgrp"/>
      <xs:enumeration value="subgrp"/>
      <xs:enumeration value="subseries"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="av.render">
    <xs:restriction base="xs:token">
      <xs:enumeration value="bold"/>
      <xs:enumeration value="italic"/>
      <xs:enumeration value="bolditalic"/>
      <xs:enumeration value="boldunderline"/>
      <xs:enumeration value="boldsmcaps"/>
      <xs:enumeration value="boldquoted"/>
      <xs:enumeration value="nonproport"/>
      <xs:enumeration value="altrender"/>
      <xs:enumeration value="quoted"/>
      <xs:enumeration value="smcaps"/>
      <xs:enumeration value="sub"/>
      <xs:enumeration value="super"/>
      <xs:enumeration value="underline"/>
    </xs:restriction>
  </xs:simpleType>
  <!--
    SOURCE ATTRIBUTE VALUES:
    
    aat      = Art and Architecture Thesaurus
    aacr2    = Anglo-American Cataloging rules, 2nd edition,
                revised
    cdwa     = Categories for the Description of Works of Art
    dot      = Dictionary of Occupational Titles
    gmgpc    = Thesaurus for Graphic Materials II
    lcnaf    = Library of Congress Name Authority File
    lcsh     = Library of Congress Subject Headings
    lctgm    = Thesaurus for Graphic Materials I
    local    = heading from a locally maintained list of
                index terms
    mesh     = Medical Subject Headings
    mim      = Moving Image Materials
    ncarules = National Council of Archives, Rules for the
                Construction of Personal, Corporate and Place
                Names
    nmc      = Revised Nomenclature for Museum Cataloging
    rad      = Rules for Archival Description
    rbgenr   = Genre Terms
    tgn      = Thesaurus of Geographic Names
    ulan     = Union List of Artist Names
    
  -->
  <xs:simpleType name="av.sources">
    <xs:restriction base="xs:token">
      <xs:enumeration value="aat"/>
      <xs:enumeration value="aacr2"/>
      <xs:enumeration value="cdwa"/>
      <xs:enumeration value="dot"/>
      <xs:enumeration value="gmgpc"/>
      <xs:enumeration value="lcnaf"/>
      <xs:enumeration value="lcsh"/>
      <xs:enumeration value="lctgm"/>
      <xs:enumeration value="local"/>
      <xs:enumeration value="mesh"/>
      <xs:enumeration value="mim"/>
      <xs:enumeration value="ncarules"/>
      <xs:enumeration value="nmc"/>
      <xs:enumeration value="rad"/>
      <xs:enumeration value="rbgenr"/>
      <xs:enumeration value="tgn"/>
      <xs:enumeration value="ulan"/>
      <xs:enumeration value="othersource"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="av.container">
    <xs:restriction base="xs:token">
      <xs:enumeration value="carton"/>
      <xs:enumeration value="box"/>
      <xs:enumeration value="folder"/>
      <xs:enumeration value="reel"/>
      <xs:enumeration value="frame"/>
      <xs:enumeration value="oversize"/>
      <xs:enumeration value="reel-frame"/>
      <xs:enumeration value="volume"/>
      <xs:enumeration value="map-case"/>
      <xs:enumeration value="box-folder"/>
      <xs:enumeration value="page"/>
      <xs:enumeration value="folio"/>
      <xs:enumeration value="othertype"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ************************************************************ -->
  <!-- COMMON ATTRIBUTES -->
  <!-- ************************************************************ -->
  <!-- Attributes used on most ead elements -->
  <xs:attributeGroup name="a.common">
    <xs:attribute name="id" type="xs:ID"/>
    <xs:attribute name="altrender"/>
    <xs:attribute name="audience">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="external"/>
          <xs:enumeration value="internal"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- ACCESS ATTRIBUTES -->
  <!-- ************************************************************ -->
  <!-- Attributes shared by all access elements. -->
  <xs:attributeGroup name="a.access">
    <xs:attribute name="source" type="av.sources"/>
    <xs:attribute name="othersource"/>
    <xs:attribute name="encodinganalog"/>
    <xs:attribute name="authfilenumber"/>
    <xs:attribute name="normal"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- DESCRIPTION ATTRIBUTES -->
  <!-- ************************************************************ -->
  <!-- Descriptive attributes shared by archdesc and c's. -->
  <xs:attributeGroup name="a.desc.base">
    <xs:attribute name="otherlevel"/>
    <xs:attribute name="langmaterial"/>
    <xs:attribute name="legalstatus">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="public"/>
          <xs:enumeration value="private"/>
          <xs:enumeration value="otherlegalstatus"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="otherlegalstatus"/>
  </xs:attributeGroup>
  <!-- Archdesc (add REQUIRED level attribute) -->
  <xs:attributeGroup name="a.desc.all">
    <xs:attribute name="level" use="required" type="av.level"/>
    <xs:attributeGroup ref="a.desc.base"/>
  </xs:attributeGroup>
  <!-- Components (add IMPLIED level attribute) -->
  <xs:attributeGroup name="a.component">
    <xs:attribute name="level" type="av.level"/>
    <xs:attributeGroup ref="a.desc.base"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- ************************************************************ -->
  <!-- LINKING AND POINTING ATTRIBUTES WITH XLINK -->
  <!-- ************************************************************ -->
  <!-- ************************************************************ -->
  <!-- ************************************************************ -->
  <!-- XLINK ATTRIBUTE SETS -->
  <!-- ************************************************************ -->
  <xs:attributeGroup name="locator.att">
    <xs:attribute name="href"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="link-semantics.att">
    <xs:attribute name="inline" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="role"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="simple-link-semantics.att">
    <xs:attribute name="inline" default="true">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="remote-resource-semantics.att">
    <xs:attribute name="role"/>
    <xs:attribute name="title"/>
    <xs:attribute name="show">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="embed"/>
          <xs:enumeration value="replace"/>
          <xs:enumeration value="new"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="actuate">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="auto"/>
          <xs:enumeration value="user"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="behavior"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="local-resource-semantics.att">
    <xs:attribute name="content-role"/>
    <xs:attribute name="content-title"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- END XLINK ATTRIBUTE SETS -->
  <!-- ************************************************************ -->
  <!-- ************************************************************ -->
  <!-- EAD LINKING AND POINTING ATTRIBUTES -->
  <!-- ************************************************************ -->
  <xs:attributeGroup name="a.internal.ptr">
    <xs:attribute name="target" type="xs:IDREF"/>
    <xs:attribute name="xpointer"/>
    <xs:attributeGroup ref="locator.att"/>
    <xs:attributeGroup ref="remote-resource-semantics.att"/>
    <xs:attributeGroup ref="local-resource-semantics.att"/>
    <xs:attributeGroup ref="simple-link-semantics.att"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="a.external.ptr">
    <xs:attribute name="entityref" type="xs:ENTITY"/>
    <xs:attribute name="xpointer"/>
    <xs:attributeGroup ref="locator.att"/>
    <xs:attributeGroup ref="remote-resource-semantics.att"/>
    <xs:attributeGroup ref="local-resource-semantics.att"/>
    <xs:attributeGroup ref="simple-link-semantics.att"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="a.linkgrp">
    <xs:attributeGroup ref="link-semantics.att"/>
    <xs:attributeGroup ref="local-resource-semantics.att"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="a.loc.internal.ptr">
    <xs:attribute name="target" type="xs:IDREF"/>
    <xs:attribute name="xpointer"/>
    <xs:attributeGroup ref="locator.att"/>
    <xs:attributeGroup ref="remote-resource-semantics.att"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="a.loc.external.ptr">
    <xs:attribute name="entityref" type="xs:ENTITY"/>
    <xs:attribute name="xpointer"/>
    <xs:attributeGroup ref="locator.att"/>
    <xs:attributeGroup ref="remote-resource-semantics.att"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- END LINKING AND POINTING ATTRIBUTES WITH XLINK -->
  <!-- ************************************************************ -->
  <!-- ************************************************************ -->
  <!-- ************************************************************ -->
  <!-- ENCODED ARCHIVAL DESCRIPTION -->
  <!-- ELEMENT DECLARATIONS -->
  <!-- ************************************************************ -->
  <!-- ************************************************************ -->
  <xs:element name="ead">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="eadheader"/>
        <xs:element minOccurs="0" ref="frontmatter"/>
        <xs:element ref="archdesc"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.ead"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.ead">
    <xs:attributeGroup ref="a.common"/>
    <xs:attribute name="relatedencoding"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- EAD HEADER -->
  <!-- ************************************************************ -->
  <xs:element name="eadheader">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="eadid"/>
        <xs:element ref="filedesc"/>
        <xs:element minOccurs="0" ref="profiledesc"/>
        <xs:element minOccurs="0" ref="revisiondesc"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.eadheader"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.eadheader">
    <xs:attributeGroup ref="a.common"/>
    <xs:attribute name="langencoding"/>
    <xs:attribute name="relatedencoding"/>
    <xs:attribute name="findaidstatus">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="unverified-partial-draft"/>
          <xs:enumeration value="unverified-full-draft"/>
          <xs:enumeration value="edited-partial-draft"/>
          <xs:enumeration value="edited-full-draft"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="encodinganalog"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- EAD ID -->
  <xs:element name="eadid">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="attlist.eadid"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.eadid">
    <xs:attribute name="systemid"/>
    <xs:attribute name="source"/>
    <xs:attribute name="type"/>
    <xs:attribute name="encodinganalog"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- FILE DESCRIPTION ELEMENTS -->
  <xs:element name="filedesc">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="titlestmt"/>
        <xs:element minOccurs="0" ref="editionstmt"/>
        <xs:element minOccurs="0" ref="publicationstmt"/>
        <xs:element minOccurs="0" ref="seriesstmt"/>
        <xs:element minOccurs="0" ref="notestmt"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.filedesc"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.filedesc">
    <xs:attributeGroup ref="a.common"/>
    <xs:attribute name="encodinganalog"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- TITLE STATEMENT -->
  <xs:element name="titlestmt">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="titleproper"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="subtitle"/>
        <xs:element minOccurs="0" ref="author"/>
        <xs:element minOccurs="0" ref="sponsor"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.titlestmt"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.titlestmt">
    <xs:attributeGroup ref="a.common"/>
    <xs:attribute name="encodinganalog"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- TITLE PROPER -->
  <xs:element name="titleproper">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="m.phrase.bare"/>
        <xs:element ref="abbr"/>
        <xs:element ref="date"/>
        <xs:element ref="expan"/>
        <xs:element ref="num"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.titleproper"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.titleproper">
    <xs:attributeGroup ref="a.common"/>
    <xs:attribute name="extent">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="all"/>
          <xs:enumeration value="part"/>
          <xs:enumeration value="unknown-extent"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="pubstatus">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="pub"/>
          <xs:enumeration value="unpub"/>
          <xs:enumeration value="unknown-pub"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="render" type="av.render"/>
    <xs:attribute name="type"/>
    <xs:attribute name="encodinganalog"/>
  </xs:attributeGroup>
  <!-- ************************************************************ -->
  <!-- SUBTITLE -->
  <xs:element name="subtitle">
